# OpenClaw Linux Agent Configuration
# Edit these values for your environment

# API endpoint
API_URL="http://192.168.0.5:8080"

# API key for authentication
API_KEY="openclaw-inventory-dev-key"

# Unique node identifier (defaults to hostname)
NODE_ID=""

# How often to push inventory (seconds)
PUSH_INTERVAL=1800

# How often to poll for jobs (seconds)
JOB_POLL_INTERVAL=60

# Log file location
LOG_FILE="/var/log/openclaw-agent.log"
